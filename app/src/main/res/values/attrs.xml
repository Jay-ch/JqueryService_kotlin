<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Top_title">
        <attr name="title" format="string" />
        <attr name="rightTitle" format="string" />
        <attr name="showLeft" format="boolean" />
        <attr name="rightRes" format="reference" />
    </declare-styleable>
    <!--自定义多种状态切换的view-->
    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="noNetworkView" format="reference" />
        <attr name="contentView" format="reference" />
    </declare-styleable>
    <declare-styleable name="Comm_RecyclerView">
        <attr name="enableRefresh" format="boolean" />
        <attr name="emptyIcon" format="reference" />
        <attr name="itemType" format="integer" />
        <attr name="itemHeight" format="integer" />
        <attr name="emptyDes" format="reference" />
        <attr name="isShowTop" format="boolean" />
    </declare-styleable>

    <!--侧边字母导航栏-->
    <declare-styleable name="WaveSideBarView">
        <attr name="sidebar_lazy_respond" format="boolean"/>
        <attr name="sidebar_text_color" format="color"/>
        <attr name="sidebar_max_offset" format="dimension"/>
        <attr name="sidebar_position" format="enum">
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
        </attr>
        <attr name="sidebar_text_alignment" format="enum">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <!--自定义搜索框-->
    <declare-styleable name="SearchEditText">
        <attr name="drawableDel" format="reference" />
    </declare-styleable>


    <declare-styleable name="searchedit">
        <attr name="imagewidth" format="dimension"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>


