<resources xmlns:tools="http://schemas.android.com/tools">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!-- <item name="android:windowTranslucentStatus" tools:ignore="NewApi">true</item>
         <item name="android:windowTranslucentNavigation" tools:ignore="NewApi">false</item>-->
        <!--Appbar背景色，应用的主要色调，actionBar默认使用该颜色-->
        <item name="colorPrimary">@color/c_ff</item>
        <item name="colorPrimaryDark">@color/c_f5</item>
        <!--<item name="android:windowAnimationStyle">@style/AnimationActivity</item>-->
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowIsTranslucent">true</item>
        <!--状态栏颜色，默认使用colorPrimaryDark-->
        <!--  <item name="statusBarColor">@color/material_animations_primary_dark</item>-->
    </style>


    <style name="AnimationActivity" >
